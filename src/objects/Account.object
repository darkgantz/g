    <validationRules>
        <fullName>Only_Ops_Manages_Trainers</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT( $User.ProfileId = &quot;00eG0000001GB2Q&quot; /*System Admin profile*/ ),
NOT( $User.ProfileId = &quot;00eG0000000S5pj&quot; /*Operations profile*/),
ISCHANGED(LastModifiedDate),
OR(Id = &apos;001G000001m505R&apos;,
Id = &apos;001G000001m6sFw&apos;,
Id = &apos;001G000001maGIe&apos;))</errorConditionFormula>
        <errorMessage>Only operations and system administrators may change SkillPath Trainer accounts.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_Changing_Ownership</fullName>
        <active>false</active>
        <errorConditionFormula>OR(
  NOT( ISPICKVAL( Owner.User_Team__c , &quot;BDRs&quot; )),
  NOT( ISPICKVAL( Owner.User_Team__c, &quot;POOL&quot; )),
  NOT( ISPICKVAL( Owner.User_Team__c, &quot;Former&quot; ))
) &amp;&amp;
 ISCHANGED(  OwnerId  )&amp;&amp; 
NOT(OR( 
$User.ProfileId = &quot;00eG0000001GB2Q&quot; /*System Admin profile*/, 
$User.ProfileId = &quot;00eG0000000S5pj&quot; /*Operations profile*/,
$User.ProfileId = &quot;00eG0000000S7n2&quot; /*Sales Managers profile*/,
$User.Id = PRIORVALUE(OwnerId)
))</errorConditionFormula>
        <errorMessage>The Ownership may not be changed due to Pool Rules.</errorMessage>
    </validationRules>
