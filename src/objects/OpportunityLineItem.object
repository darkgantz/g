    <validationRules>
        <fullName>Cannot_change_name_unless_Custom</fullName>
        <active>true</active>
        <description>If the Opportunity record type is not Custom, the user can&apos;t change the Name of the topic/session.</description>
        <errorConditionFormula>NOT(Opportunity.RecordType.Id= &quot;012G0000001QN3Y&quot; ) 
 &amp;&amp;   ISCHANGED( Topic_Name__c )</errorConditionFormula>
        <errorMessage>Opportunity record type must be Custom to have a different name applied. Please change the record type before making the name change.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>no_Zero_amount_opp_products</fullName>
        <active>true</active>
        <errorConditionFormula>AND(UnitPrice =0, 
$Setup.VRS__c.no_Zero_amount_opp_products__c = False)</errorConditionFormula>
        <errorMessage>Only Managers or Operations can enter $0 Products.</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>Add_Product</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Add Product</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>false</requireRowSelection>
        <url>var redirectTo = &quot;{!IF(ISNULL(Opportunity.PricebookIDforFormula__c ),&apos;oppitm/choosepricebook.jsp?id=&apos; + Opportunity.Id + &apos;&amp;retURL=&apos; + URLENCODE( &apos;/&apos; + Opportunity.Id) + &apos;&amp;saveURL=&apos; + URLENCODE(&apos;/apex/ProductSelector?Id=&apos; + Opportunity.Id)
,&apos;/apex/ProductSelector?Id=&apos;+ Opportunity.Id)}&quot;;

window.top.location.href = redirectTo;</url>
    </webLinks>
