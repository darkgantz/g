        <description>If the record type is a Content License, prevent the closed won until NDA is added to the Account</description>
        <errorConditionFormula>ISNULL( Combined_NDA_Received_Date__c ) 
&amp;&amp; 
IsWon =TRUE
&amp;&amp; 
RecordTypeId = &quot;01241000001E9XT&quot; /*License Record Type*/
&amp;&amp;
NOT( $User.ProfileId = &quot;00e41000001HG59&quot; /*System Admin profile*/ )
&amp;&amp;
NOT( $User.ProfileId = &quot;00e41000000Ga1k&quot; /*Operations profile*/)</errorConditionFormula>
        <errorMessage>Please update the Account record with the NDA received date before moving to Closed-Won.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_Changing_Ownership</fullName>
        <active>true</active>
        <errorConditionFormula>OR( 
NOT( ISPICKVAL( Owner.User_Team__c  , &quot;BDRs&quot; )), 
NOT( ISPICKVAL( Owner.User_Team__c, &quot;POOL&quot; )), 
NOT( ISPICKVAL( Owner.User_Team__c, &quot;Former&quot; )) 
) &amp;&amp; 
ISCHANGED(  OwnerId  )&amp;&amp; 
NOT(OR(
$User.ProfileId = &quot;00e41000001HG59&quot; /*System Admin profile*/,
$User.ProfileId = &quot;00e41000000Ga1k&quot; /*Operations profile*/,
$User.ProfileId = &quot;00e41000000Ga1l&quot; /*Sales Managers*/,
$User.Id = PRIORVALUE(OwnerId)
))</errorConditionFormula>
        <errorMessage>The Ownership may not be changed due to Pool Rules.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Shipping_Contact_Needs_Address_Data</fullName>
