&amp;&amp;
Number_of_Custom_Content_Items__c = 0
&amp;&amp;
RecordTypeId = &quot;012G0000001QN3Y&quot; /* Custom Opportunity Record Type */
&amp;&amp;
NOT( $User.ProfileId = &quot;00eG0000001GB2Q&quot; /*System Admin profile*/ )
&amp;&amp;
NOT( $User.ProfileId = &quot;00eG0000000S5pj&quot; /*Operations profile*/)</errorConditionFormula>
        <errorMessage>Please enter a custom content item for this Opportunity. A Custom Record Type cannot be Closed without a Custom Content Item record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lock_Onsite_Opportunity</fullName>
        <active>false</active>
        <description>Once the Opp is converted, prevent the user from editing the record.</description>
        <errorConditionFormula>PRIORVALUE(IsWon) 
&amp;&amp; 
(IsWon || NOT(IsWon)) 
&amp;&amp; 
NOT( $User.ProfileId = &quot;00e41000001HG59&quot;) /*System Administrator*/ 
&amp;&amp; 
NOT( $User.ProfileId = &quot;00e41000000Ga1k&quot; /*Operations profile*/) 
&amp;&amp; 
NOT(ISCHANGED(Number_of_TE_Days_Created__c)) 
&amp;&amp; 
NOT(RecordType.Name=&quot;On-site Unlocked&quot;) 
&amp;&amp; 
NOT(ISCHANGED(TE_count_dlrs__c))</errorConditionFormula>
        <errorMessage>Only System Administrators &amp; Operations can make changes to Locked Opportunities.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>On_site_Won_deals_require_detailed_info</fullName>
        <active>false</active>
        <description>Must have Account Industry, Billing Address info, Opportunity Contact Details, Training Start Date, Times, and Max Attendees, Ship and Location Info.</description>
        <errorConditionFormula>((RecordType.DeveloperName = &quot;On_site_Standard&quot; ||
RecordType.DeveloperName = &quot;On_site_Custom&quot; ) &amp;&amp; IsWon = TRUE &amp;&amp;
OR(
ISPICKVAL( Account.Industry,&quot;&quot; ),
ISBLANK( Account.BillingAddress ),
ISBLANK( Account.BillingCity ),
ISBLANK( Account.BillingState),
ISBLANK( Account.BillingPostalCode ),
ISBLANK( Billing_Contact__c ),
ISBLANK( Decision_Maker__c ),
ISBLANK( Shipping_Contact__c ),
ISBLANK( Shipping_Attention_Line__c ),
ISBLANK( Training_Location_Contact__c ),
ISBLANK( Max_Attendees__c ) ,
ISBLANK( TrainingStartDate__c ),
ISPICKVAL( Start_Time__c, &quot;&quot; ),
ISPICKVAL( End_Time__c, &quot;&quot; ),
ISBLANK( Billing_Currency__c )
))

||

(RecordType.DeveloperName = &quot;On_site_Webinar&quot;
&amp;&amp; IsWon = TRUE &amp;&amp;
OR(
ISPICKVAL( Account.Industry,&quot;&quot; ),
ISBLANK( Account.BillingAddress ),
ISBLANK( Account.BillingCity ),
ISBLANK( Account.BillingState),
ISBLANK( Account.BillingPostalCode ),
ISBLANK( Billing_Contact__c ),
ISBLANK( Decision_Maker__c ),
ISBLANK( Max_Attendees__c ) ,
ISBLANK( TrainingStartDate__c ),
ISPICKVAL( Start_Time__c, &quot;&quot; ),
ISPICKVAL( End_Time__c, &quot;&quot; ),
ISBLANK( Billing_Currency__c ),
ISBLANK( Webinar_Attendees_Per_Session__c ),
ISPICKVAL( Webinar_Hosting__c, &quot;&quot; ),
ISPICKVAL( Webinar_Platform__c, &quot;&quot; ),
ISBLANK( Webinar_Session_Times__c ),
ISPICKVAL( Webinar_Time_Zone__c, &quot;&quot; )
))

||

(RecordType.DeveloperName = &quot;On_site_License&quot; &amp;&amp; IsWon = TRUE &amp;&amp;
OR(
ISPICKVAL( Account.Industry,&quot;&quot; ),
ISBLANK( Account.BillingAddress ),
ISBLANK( Account.BillingCity ),
ISBLANK( Account.BillingState),
ISBLANK( Account.BillingPostalCode ),
ISBLANK( Billing_Contact__c ),
ISBLANK( Decision_Maker__c ),
ISBLANK( Shipping_Contact__c ),
ISBLANK( Training_Location_Contact__c ),
ISBLANK( Max_Attendees__c ),
ISBLANK( Billing_Currency__c )
))</errorConditionFormula>
        <errorMessage>REQUIRED: ACCOUNT Industry &amp; Billing Address, ALL OPPORTUNITIES - DM, Billing Contact, Max # Attendees, ILT - Shipping Contact, Training Contact, ATTN line, Training Start Date &amp; Times, WEBINARS - Host and Platform fields.</errorMessage>
    </validationRules>
