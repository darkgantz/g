        <errorConditionFormula>AND(
NOT( $User.ProfileId = &quot;00e41000001HG59&quot; /*System Admin profile*/ ),
NOT( $User.ProfileId = &quot;00e41000000Ga1k&quot; /*Operations profile*/),
NOT(ISBLANK( Training_Location_Contact__c )),
OR(
ISBLANK( Training_Location_Contact__r.MailingStreet ),
ISBLANK( Training_Location_Contact__r.MailingCity ),
ISBLANK( Training_Location_Contact__r.MailingState ),
