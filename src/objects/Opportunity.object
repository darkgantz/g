        <active>true</active>
        <errorConditionFormula>IsWon = true 
&amp;&amp; 
ISBLANK( Training_Location_Tax__c )
&amp;&amp; 
NOT( $User.ProfileId = &quot;00eG0000001GB2Q&quot; /*System Admin profile*/ ) 
&amp;&amp; 
NOT( $User.ProfileId = &quot;00eG0000000S5pj&quot; /*Operations profile*/)</errorConditionFormula>
        <errorDisplayField>Training_Location_Tax__c</errorDisplayField>
        <errorMessage>To win an opportunity, you must have a Training Location Tax.</errorMessage>
